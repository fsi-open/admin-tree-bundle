{
    "name": "fsi/admin-tree-bundle",
    "type": "symfony-bundle",
    "description": "Provides integration for fsi/admin-bundle with gedmo/doctrine-extensions tree.",
    "keywords": ["admin", "symfony", "bundle", "tree", "fsi", "gedmo", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kapera",
            "email": "piotr.kapera@fsi.pl"
        }
    ],
    "require": {
        "fsi/admin-bundle" : "~2.0|~3.0",
        "gedmo/doctrine-extensions": "~2.4",
        "php": ">=7.1",
        "symfony/config" : "~2.3,>=2.3.12|~3.0|~4.0",
        "symfony/dependency-injection" : "~2.3|~3.0|~4.0",
        "symfony/framework-bundle" : "~2.3|~3.0|~4.0",
        "symfony/http-foundation" : "~2.3,>=2.3.19|~3.0|~4.0",
        "symfony/http-kernel" : "~2.3,>=2.3.22|~3.0|~4.0",
        "symfony/routing" : "~2.3|~3.0|~4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~4.3",
        "phpspec/prophecy": "~1.7"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminTreeBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "1.0": "1.0-dev"
        }
    }
}
