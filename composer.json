{
    "name": "fsi/admin-tree-bundle",
    "type": "symfony-bundle",
    "description": "Provides integration for fsi/admin-bundle with gedmo/doctrine-extensions tree.",
    "keywords": ["admin", "symfony", "bundle", "tree", "fsi", "gedmo", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kapera",
            "email": "piotr.kapera@fsi.pl"
        }
    ],
    "require": {
        "doctrine/orm": "^2.5",
        "fsi/admin-bundle" : "^3.1",
        "gedmo/doctrine-extensions": "^2.4",
        "php": ">=7.1",
        "symfony/config" : "^3.4|^4.0",
        "symfony/dependency-injection" : "^3.4|^4.0",
        "symfony/framework-bundle" : "^3.4|^4.0",
        "symfony/http-foundation" : "^3.4|^4.0",
        "symfony/http-kernel" : "^3.4|^4.0",
        "symfony/routing" : "^3.4|^4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.0|^6.0",
        "phpdocumentor/reflection-docblock": "^3.1"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminTreeBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\FSi\\Bundle\\AdminTreeBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev",
            "2.0": "2.0-dev",
            "1.0": "1.0-dev"
        }
    }
}
