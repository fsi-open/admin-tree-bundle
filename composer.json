{
    "name": "fsi/admin-tree-bundle",
    "type": "symfony-bundle",
    "description": "FSi admin tree bundle.",
    "keywords": ["admin", "symfony2", "bundle", "tree"],
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kapera",
            "email": "piotr.kapera@fsi.pl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle" : "^2.3",
        "fsi/admin-bundle" : "~1.1@dev",
        "gedmo/doctrine-extensions": "2.3.*"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FSi\\Bundle\\AdminTreeBundle\\": ""
        }
    },
    "target-dir" : "FSi/Bundle/AdminTreeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
